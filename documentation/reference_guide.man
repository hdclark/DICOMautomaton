\f[C]"all"\f[]
.IP \[bu] 2
\f[C]"none"\f[]
.IP \[bu] 2
\f[C]"#0"\f[]
.IP \[bu] 2
\f[C]"#\-0"\f[]
.IP \[bu] 2
\f[C]"!last"\f[]
.IP \[bu] 2
\f[C]"!#\-3"\f[]
.IP \[bu] 2
\f[C]"key\@.*value.*"\f[]
.IP \[bu] 2
\f[C]"key1\@.*value1.*;key2\@^value2$;first"\f[]
